<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="@drawable/selector_function_item_tv_bg"
    android:layout_marginTop="10dp"
    android:layout_marginLeft="15dp"
    android:layout_marginRight="15dp"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/tv_name"
        android:text=""
        android:textStyle="bold"
        android:textSize="16sp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="21dp"
        android:textColor="@color/color_010101"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/tv_x_px"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="16dp"
        android:layout_marginBottom="22dp"
        android:layout_below="@+id/tv_name"
        android:text="30"
        android:textSize="12sp"
        android:textColor="@color/color_A6A6A6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/tv_x"
        android:layout_toRightOf="@+id/tv_x_px"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="22dp"
        android:layout_below="@+id/tv_name"
        android:text=" x "
        android:textSize="12sp"
        android:textColor="@color/color_A6A6A6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/tv_y_px"
        android:layout_toRightOf="@+id/tv_x"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="22dp"
        android:layout_below="@+id/tv_name"
        android:text="40"
        android:textSize="12sp"
        android:textColor="@color/color_A6A6A6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/tv_px"
        android:layout_toRightOf="@+id/tv_y_px"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="22dp"
        android:layout_below="@+id/tv_name"
        android:text="px"
        android:textSize="12sp"
        android:textColor="@color/color_A6A6A6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/tv_x_mm"
        android:layout_marginLeft="16dp"
        android:layout_alignTop="@+id/tv_x_px"
        android:layout_toRightOf="@+id/tv_px"
        android:text="25"
        android:textSize="12sp"
        android:textColor="@color/color_A6A6A6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/tv_x2"
        android:layout_alignTop="@+id/tv_x_px"
        android:layout_toRightOf="@+id/tv_x_mm"
        android:text=" x "
        android:textSize="12sp"
        android:textColor="@color/color_A6A6A6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/tv_y_mm"
        android:layout_alignTop="@+id/tv_x_px"
        android:layout_toRightOf="@+id/tv_x2"
        android:text="35"
        android:textSize="12sp"
        android:textColor="@color/color_A6A6A6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/tv_mm"
        android:layout_alignTop="@+id/tv_x_px"
        android:layout_toRightOf="@+id/tv_y_mm"
        android:text="mm"
        android:textSize="12sp"
        android:textColor="@color/color_A6A6A6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/tv_kb"
        android:maxLines="1"
        android:ellipsize="end"
        android:layout_marginLeft="16dp"
        android:layout_alignTop="@+id/tv_x_px"
        android:layout_toRightOf="@+id/tv_mm"
        android:text=""
        android:textSize="12sp"
        android:textColor="@color/color_A6A6A6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <ImageView
        android:layout_marginRight="16dp"
        android:layout_centerVertical="true"
        android:layout_alignParentRight="true"
        android:src="@drawable/xiangji_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
</RelativeLayout>